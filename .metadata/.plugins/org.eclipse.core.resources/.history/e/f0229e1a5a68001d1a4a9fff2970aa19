package com.keilacouto.provanivel2.enums;

public enum EnumStatusPedido {

	PRODUTO("Ativo"),
    SERVICO("Inativo");

    private String descricao;

    EnumStatusPedido(String descricao) {
        this.descricao = descricao;
    }

    public String getDescricao() {
        return descricao;
    }
}
